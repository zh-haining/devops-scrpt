pipeline {
   agent any
   parameters {
      string(defaultValue: 'master', description: '分支名称', name: 'branch_name', trim: true)
      choice(choices: ['test-9091', 'test9092', 'pre-9080'], description: '环境', name: 'env-type')
   }
   stages {
      stage('pull code') {
         steps {
            checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: '57816bd2-915c-449f-9614-e83767cb419f', url: 'https://github.com/zh-haining/jenkins-demo.git']]])
         }
      }
      stage('build project') {
         steps {
            sh 'mvn clean package -Dmaven.test.skip=true'
         }
      }
   }
}
